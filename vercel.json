{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/subscribe",
      "dest": "index.js"
    },
    {
      "src": "/api/unsubscribe",
      "dest": "index.js"
    },
    {
      "src": "/api/send-mass-email",
      "dest": "index.js"
    },
    {
      "src": "/api/subscriber-count",
      "dest": "index.js"
    },
    {
      "src": "/health",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}
